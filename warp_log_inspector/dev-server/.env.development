# Warp Telemetry Development Server Configuration

# Server Configuration
PORT=3001
NODE_ENV=development

# RudderStack Configuration  
RUDDERSTACK_DATA_PLANE_URL=http://localhost:3001
RUDDERSTACK_WRITE_KEY=your_write_key_here

# Feature Flags
ENABLE_WEBSOCKET=false
ENABLE_TELEMETRY_INTERCEPTION=true
ENABLE_TELEMETRY_VALIDATION=true
ENABLE_TELEMETRY_ENRICHMENT=true

# Logging
LOG_LEVEL=debug

# Session Configuration
MAX_BATCH_SIZE=100
FLUSH_INTERVAL=30000
MAX_SESSION_TIME=1800000
